{
    "name": "alishadel/bitly-shortener",
    "description": "Shorten Long Urls Using Bitly Api",
    "type": "library",
    "license": "MIT",
    "authors": [
        {
            "name": "alishadel2001",
            "email": "alishadel2001@gmail.com"
        }
    ],
    "minimum-stability": "dev",
    "require": {
        "guzzlehttp/guzzle": "^7.0"
    },
    "autoload": {
        "psr-4": {
            "BitlyShortener\\": "src/"
        },
        "files": [
            "src/helpers.php"
        ]
    },
    "extra": {
        "laravel": {
            "providers": [
                "BitlyShortener\\BitlyServicesProvider"
            ],
            "aliases": {
                "Bitly": "BitlyShortener\\Bitly"
            }
        }
    }
}